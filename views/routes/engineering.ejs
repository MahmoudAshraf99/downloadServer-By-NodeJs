<%- include('../partilas/header'); -%>
</section>
<section id="posts">
  <div class="container-fluid">
    <div class="row">
      <%posts.forEach((post)=>{ %>
                <div class="col-lg-4 col-md-6">
                    <a href="/engineering/<%= post._id %>">
                        <div class="card"  >
                            <img src="uploads/<%=post.imgFileName%>"  class="card-img-top" alt="...">
                                  
                            <div class="card-body">
                                <h5 class="card-title"><%= post.title %></h5>
                            </div>
                        </div>
                    </a>
                  </div>
    <%})%>
    </div>
    <%- include('pagination', {currentPage: currentPage, previousPage: previousPage, hasPreviousPage:hasPreviousPage, hasNextPage:hasNextPage, nextPage:nextPage, lastPage:lastPage}); -%>
  </div>
</section>
<script src="https://propu.sh/pfe/current/tag.min.js?z=3777834" data-cfasync="false" async></script>
<%- include('../partilas/footer'); -%>